{
    "title": "Test Management",
    "subtitle": "Manage tests in the system",
    "description": "Test management system with CRUD operations.",
    "route": {
        "name": "admin.test",
        "prefix": "admin.test",
        "actions": {
            "index": "index",
            "create": "create",
            "store": "store",
            "show": "show",
            "edit": "edit",
            "update": "update",
            "destroy": "destroy"
        }
    },
    "table": {
        "name": "admin_tests",
        "model": "\\Jiny\\Admin2\\App\\Models\\AdminTest",
        "primaryKey": "id",
        "timestamps": true,
        "softDeletes": false,
        "where": {
            "default": [],
            "conditions": []
        },
        "casts": {
            "enable": "boolean"
        }
    },
    "template": {
        "layout": "jiny-admin2::layouts.admin",
        "index": "jiny-admin2::template.index",
        "show": "jiny-admin2::template.show",
        "create": "jiny-admin2::template.create",
        "edit": "jiny-admin2::template.edit"
    },
    "index": {
        "tableLayoutPath": "jiny-admin2::template.livewire.admin-table",
        "tablePath": "jiny-admin2::admin.admin_test.table",
        "searchLayoutPath": "jiny-admin2::template.livewire.admin-search",
        "searchFormPath": "jiny-admin2::admin.admin_test.search",
        "paging": 20,
        "heading": {
            "title": "Test List2",
            "description": "Manage your tests..."
        },
        "features": {
            "enableCreate": true,
            "enableDelete": true,
            "enableEdit": true,
            "enableSearch": true,
            "enableSort": true,
            "enablePagination": true,
            "enableBulkActions": true,
            "enableExport": false,
            "enableImport": false,
            "enableStatusToggle": true,
            "enableSettingsDrawer": true
        },
        "pagination": {
            "perPage": "10",
            "perPageOptions": [
                10,
                25,
                50,
                100
            ]
        },
        "sorting": {
            "default": "created_at",
            "direction": "desc"
        },
        "search": {
            "placeholder": "Search tests...",
            "debounce": 300
        }
    },
    "create": {
        "buttonText": "Add New Test",
        "heading": {
            "title": "Create New Test",
            "description": "Add a new test to the system"
        },
        "enableContinueCreate": true,
        "enableListButton": true,
        "enableSettingsDrawer": true,
        "createLayoutPath": "jiny-admin2::template.livewire.admin-create",
        "formPath": "jiny-admin2::admin.admin_test.create",
        "formLayout": "vertical"
    },
    "edit": {
        "heading": {
            "title": "Edit Test",
            "description": "Update test information"
        },
        "enableDelete": true,
        "enableListButton": true,
        "enableDetailButton": true,
        "enableSettingsDrawer": true,
        "editLayoutPath": "jiny-admin2::template.livewire.admin-edit",
        "formPath": "jiny-admin2::admin.admin_test.edit",
        "formLayout": "vertical"
    },
    "show": {
        "heading": {
            "title": "Test Details",
            "description": "View test information"
        },
        "enableEdit": true,
        "enableDelete": true,
        "enableCreate": true,
        "enableListButton": true,
        "enableSettingsDrawer": true,
        "showLayoutPath": "jiny-admin2::template.livewire.admin-show",
        "showPath": "jiny-admin2::admin.admin_test.show",
        "display": {
            "dateFormat": "Y-m-d H:i:s",
            "booleanLabels": {
                "true": "Enabled",
                "false": "Disabled"
            }
        }
    },
    "destroy": {
        "enableSoftDelete": false,
        "enableTransaction": true,
        "requireConfirmation": true,
        "confirmation": {
            "message": "Are you sure you want to delete this test?"
        },
        "messages": {
            "success": "Test deleted successfully.",
            "error": "Error deleting test: %s",
            "confirmRequired": "Delete confirmation required."
        }
    },
    "store": {
        "enableTransaction": true,
        "defaults": {
            "enable": true,
            "pos": 0,
            "depth": 0,
            "ref": 0
        },
        "fillable": [
            "enable",
            "title",
            "description",
            "pos",
            "depth",
            "ref"
        ],
        "messages": {
            "success": "Test created successfully.",
            "error": "Error creating test.",
            "continueSuccess": "Test created. You can continue adding more."
        },
        "validation": {
            "rules": {
                "title": "required|string|max:255",
                "description": "nullable|string",
                "enable": "boolean",
                "pos": "nullable|integer",
                "depth": "nullable|integer",
                "ref": "nullable|integer"
            },
            "messages": {
                "title.required": "Title is required."
            }
        }
    },
    "update": {
        "enableTransaction": true,
        "trackChanges": true,
        "redirects": {
            "success": "show",
            "error": "edit"
        },
        "fillable": [
            "enable",
            "title",
            "description",
            "pos",
            "depth",
            "ref"
        ],
        "messages": {
            "success": "Test updated successfully.",
            "error": "Error updating test."
        },
        "validation": {
            "rules": {
                "title": "required|string|max:255",
                "description": "nullable|string",
                "enable": "boolean",
                "pos": "nullable|integer",
                "depth": "nullable|integer",
                "ref": "nullable|integer"
            },
            "messages": {
                "title.required": "Title is required."
            }
        }
    }
}